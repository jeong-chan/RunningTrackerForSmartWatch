<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#ffecb3">


    <EditText
        android:id="@+id/end_date"
        android:layout_width="159dp"
        android:layout_height="47dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="124dp"
        android:cursorVisible="false"
        android:ems="10"
        android:gravity="center"
        android:inputType="none"
        android:text="종료일"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.157"
        app:layout_constraintStart_toEndOf="@+id/start_date"
        app:layout_constraintTop_toTopOf="parent"
        android:textSize="16dp"
        />

    <EditText
        android:id="@+id/start_date"
        android:layout_width="171dp"
        android:layout_height="50dp"
        android:layout_marginTop="124dp"
        android:cursorVisible="false"
        android:ems="10"
        android:gravity="center"
        android:inputType="none"
        android:text="시작일"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.112"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:textSize="16dp"/>

    <EditText
        android:id="@+id/timedata"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="240dp"
        android:cursorVisible="false"
        android:ems="10"
        android:gravity="center"
        android:inputType="none"
        android:text="time"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:textSize="16dp"/>

    <EditText
        android:id="@+id/disdata"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="none"
        android:text="distance"
        android:cursorVisible="false"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="@+id/timedata"
        app:layout_constraintStart_toStartOf="@+id/timedata"
        app:layout_constraintTop_toBottomOf="@+id/timedata"
        android:textSize="16dp"/>

    <EditText
        android:id="@+id/kcaldata"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="none"
        android:cursorVisible="false"
        android:text="kcal"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="@+id/disdata"
        app:layout_constraintStart_toStartOf="@+id/disdata"
        app:layout_constraintTop_toBottomOf="@+id/disdata"
        android:textSize="16dp"/>




</androidx.constraintlayout.widget.ConstraintLayout>